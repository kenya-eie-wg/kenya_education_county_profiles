) %>%
select(-national_average) %>%
mutate_at(vars(indicator), ~ str_trim(str_squish(.))) %>%
mutate_at(vars(indicator),
~ gsub("\\[.*?\\]", "", .)) %>%
filter(!is.na(indicator) & !str_detect(indicator, "WASH in")) %>%
pivot_longer(cols = c(preprimary_ece_female:secondary_total),
